<Window x:Class="ChatWindowsClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatWindowsClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="MainWindow_Loaded">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="62,383,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="19,379,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Message" Content="Label" HorizontalAlignment="Left" Margin="250,379,0,0" VerticalAlignment="Top" Width="65"/>
        <TextBox x:Name="MessageBox" HorizontalAlignment="Left" Margin="362,392,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="526,392,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <TextBox x:Name="chatMessages" HorizontalAlignment="Left" Margin="19,29,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="495" Height="259" />

    </Grid>
</Window>
